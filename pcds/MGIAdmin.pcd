                                        text = scrolledText;
                                        Group: Widget
                                          rows = 15;
                                          columns = 125;
                                      }
                                }
                                {
                                  Columns = Table : mgiTable;
                                  Group: Constraint
                                    leftAttachment = attach_opposite_widget;
                                    leftWidget = Description;
                                    rightAttachment = attach_none;
                                    topAttachment = attach_widget;
                                    topWidget = Description;
                                    topOffset = 5;
                                    bottomAttachment = attach_form;
                                    resizable = true;
                                    bottomOffset = 5;
                                      {
                                        Table = mgiTable;
                                        Group: Widget
                                          xrtTblPixelWidthSeries = "(all 0-1 0)";
                                          xrtTblColumnLabels = "Mode,Key,Column,Description,Example";
                                          xrtTblCharWidthSeries = "(all 0 1)(all 2 20)(all 3-4 51)";
                                          xrtTblCharHeightSeries = "(allcells all 5)";
                                          xrtTblMaxLengthSeries = "(all 2 20)(all 3-4 255)";
                                          xrtTblEditableSeries = "(all 0-1 False)";
                                          xrtTblTraversableSeries = "(all 0-1 False)";
                                          userDefinedAttributes = "columnKey:Int(Int)=\"1\"\ncolumnName:Int(Int)=\"2\"\ndescription:Int(Int)=\"3\"\nexample:Int(Int)=\"4\"\n";
                                          xrtTblWordWrapSeries = "(allcells 3-4 True)";
                                      }
                                      {
                                        AddRow = TablePush;
                                        Group: Widget
                                          managed = False;
                                      }
                                      {
                                        DeleteRow = TablePush;
                                        Group: Widget
                                          managed = False;
                                      }
                                }
                          }
                    }
                    {
                      Control = Control;
                          {
                            ControlForm = XmForm;
                                {
                                  Search = ControlPush;
                                }
                                {
                                  Clear = ControlPush;
                                }
                                {
                                  Modify = ControlPush;
                                }
                                {
                                  Add = ControlPush;
                                  Group: Widget
                                    sensitive = False;
                                }
                                {
                                  Delete = ControlPush;
                                  Group: Widget
                                    sensitive = False;
                                }
                          }
                    }
              }
        }
