                    }
                    {
                      Yes = Yes : mgiOption;
                      Group: Widget
                        valueChangedCallback = "D:SetModify";
                    }
                    {
                      No = No : mgiOption;
                      Group: Widget
                        selectColor = Green;
                        valueChangedCallback = "D:SetModify";
                    }
              }
        }
}

