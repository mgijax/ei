ClipboardAdd [clipboard : widget;
	      item : string;
	      key : string;
	      accID : string := "";];

ClipboardClear [source_widget : widget;];

ClipboardDelete [source_widget : widget;];

ClipboardSort [source_widget : widget;];

ClipboardLoad [source_widget : widget;
	       allowDups : boolean := false;];

